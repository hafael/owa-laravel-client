{
    "name": "hafael/owa-laravel-client",
    "type": "package",
    "description": "Laravel 5.3+ integration for the Open Web Analytics PHP REST Client package.",
    "keywords": [ "laravel", "open web analytics", "php", "client", "open", "web", "analytics", "rest", "api", "open source" ],
    "homepage": "https://github.com/hafael/owa-laravel-client",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "VerdeIT",
            "email": "villa655321verde@gmail.com",
            "homepage": "https://github.com/hafael"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.3.0",
        "hafael/owa-php-client": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Hafael\\OpenWebAnalytics\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/OpenWebAnalyticsLaravelTest.php"
        ],
        "psr-4": {
            "Hafael\\OpenWebAnalytics\\Laravel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
